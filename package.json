{
  "name": "wellspring",
  "description": "WordPress progressive web app theme based on React.js",
  "version": "1.0.0",
  "homepage": "https://belugathemes.com/wellspring",
  "author": {
    "name": "Beluga Themes",
    "url": "https://belugathemes.com"
  },
  "contributors": {
    "name": "Radek Stangel",
    "url": "https://radekstangel.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/radekstangel/wellspring.git"
  },
  "scripts": {
    "start": "npm run vendor && webpack --watch -d",
    "dist": "npm run vendor && webpack -p",
    "vendor": "mkdir -p dist/vendor && cp node_modules/react/dist/react.min.js node_modules/react-dom/dist/react-dom.min.js dist/vendor"
  },
  "dependencies": {
    "jquery": "^3.1.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-router": "^2.7.0"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "css-loader": "^0.24.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "node-sass": "^3.8.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-livereload-plugin": "^0.8.2"
  },
  "private": true
}
